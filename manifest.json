{
  "name": "Active Internet",
  "manifest_version": 3,
  "version": "0.1",
  "description": "",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Toggle Extension"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
